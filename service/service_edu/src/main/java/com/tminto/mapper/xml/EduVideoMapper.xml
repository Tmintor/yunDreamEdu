<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.tminto.mapper.EduVideoMapper">

    <select id="selectAllVodIdByCourseId" resultType="java.lang.String">
        select video_source_id
        from edu_video
        where course_id = #{id};
    </select>
</mapper>
